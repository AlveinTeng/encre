{
  "name": "encre-core",
  "version": "0.0.1",
  "main": "./index.js",
  "repository": "https://github.com/VictorS67/encre",
  "author": "Encre",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "yarn build",
    "build": "yarn clean && yarn build:esm && node scripts/create-entrypoints.js && node scripts/check-tree-shaking.js",
    "build:esm": "tsc --outDir build/ && rimraf build/tests build/**/tests && cp .env.local ./build/.env && cp .env.local ./build/.env.local",
    "build:watch": "node scripts/create-entrypoints.js && tsc --outDir build/ --watch",
    "clean": "rimraf build/ && node scripts/create-entrypoints.js pre",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "test": "jest --testPathIgnorePatterns=\\.int\\.test.ts --testTimeout 30000 --maxWorkers=50%",
    "test:single": "yarn run jest --config jest.config.cjs --testTimeout 100000"
  },
  "packageManager": "yarn@4.0.0-rc.45",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@swc/core": "^1.3.93",
    "@swc/jest": "^0.2.29",
    "@types/eslint": "^8.44.2",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.8.6",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "8.48.0",
    "eslint-cjs-to-esm": "^2.2.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rollup": "^4.1.4",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.2"
  },
  "dependencies": {
    "@types/pdf-parse": "^1.1.2",
    "camelcase": "^8.0.0",
    "decamelize": "^6.0.0",
    "langchain": "^0.0.149",
    "nanoid": "^5.0.2",
    "openai": "^4.7.0",
    "p-queue": "^7.4.1",
    "pdf-parse": "^1.1.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "exports": {
    "./load": {
      "types": "./load.d.ts",
      "import": "./load.js",
      "require": "./load.cjs"
    },
    "./load/serializable": {
      "types": "./load/serializable.d.ts",
      "import": "./load/serializable.js",
      "require": "./load/serializable.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "build/",
    "load.cjs",
    "load.js",
    "load.d.ts",
    "load/serializable.cjs",
    "load/serializable.js",
    "load/serializable.d.ts",
    "index.cjs",
    "index.js",
    "index.d.ts"
  ]
}
